<strong>Position: </strong><%= @project.title %><br>
<img src="<%= @project.image_url %>" height="150"><br>
<strong>Information: </strong><%= @project.information %><br><br>

<div class="btn-group">
    <button onclick="document.location='/projects/<%= @project.id %>/edit'">Edit Project</button>
    <form action="/projects/<%= @project.id %>/delete" method="post">
        <input id="hidden" type="hidden" name="_method" value="delete">
        <button>Delete Project</button>
    </form>
</div>